{
  "expo": {
    "name": "AuctionExpoApp",
    "slug": "AuctionExpoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aditya8766.AuctionExpoApp",
      "enableHermes": true
    },
    "extra": {
      "eas": {
        "projectId": "29edf90e-2f32-4178-b957-e9b565b4be76"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false
    },
    "plugins": [
      "expo-image-picker"
    ]
  }
}
